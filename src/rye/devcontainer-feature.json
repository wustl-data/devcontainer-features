{
    "name": "Rye",
    "id": "rye",
    "version": "1.0.10",
    "description": "A New Kind Of Package Management Solution for Python",
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "containerEnv": {
        "RYE_INSTALL_OPTION": "--yes",
        "RYE_HOME": "/usr/local/share/rye",
        "RYE_TOOLCHAIN": "/usr/local/bin/python"
    },
    "postCreateCommand": "sudo mv /usr/local/share/rye/env /usr/local/bin/rye"
}
