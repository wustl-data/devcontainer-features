{
    "name": "Rye",
    "id": "rye",
    "version": "1.0.2",
    "description": "A New Kind Of Package Management Solution for Python",
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "containerEnv": {
        "RYE_INSTALL_OPTION": "--yes"
    },
    "postCreateCommand": "echo 'source $HOME/.rye/env' >> ~/.profile",
    "postAttachCommand": "${localEnv:HOME}/.rye/shims/rye sync"
    
}
