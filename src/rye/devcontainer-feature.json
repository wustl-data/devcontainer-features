{
    "name": "Rye",
    "id": "rye",
    "version": "1.0.7",
    "description": "A New Kind Of Package Management Solution for Python",
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "containerEnv": {
        "RYE_INSTALL_OPTION": "--yes",
        "RYE_HOME": "/opt/rye"
    },
    "postCreateCommand": "echo 'source /opt/rye/env' >> ${containerEnv:HOME}/.profile"
}
